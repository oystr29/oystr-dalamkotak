{
  "name": "oystr-dalamkotak",
  "type": "module",
  "private": true,
  "scripts": {
    "astro": "astro",
    "sync": "astro sync",
    "build": "bun run fix && astro build",
    "check": "bun run check.fmt && bun run check.lint && astro check",
    "check.fmt": "prettier --check .",
    "check.lint": "eslint \"**/*.{js,jsx,ts,tsx,astro}\"",
    "dev": "astro dev",
    "fix": "bun run lint && bun run fmt",
    "fmt": "prettier --write .",
    "lint": "eslint --fix \"**/*.{js,jsx,ts,tsx,astro}\"",
    "lint.dry-run": "eslint --fix-dry-run \"**/*.{js,jsx,ts,tsx,astro}\"",
    "preview": "bun run build && astro preview",
    "prod": "bun run check && astro build",
    "start": "astro dev --open"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/solid-js": "^5.1.0",
    "@builder.io/qwik": "^1.12",
    "@formkit/auto-animate": "^0.8.2",
    "@qwikdev/astro": "^0.7",
    "@tailwindcss/vite": "^4.0.17",
    "astro": "^5.1",
    "embla-carousel": "^8.5.2",
    "embla-carousel-solid": "^8.5.2",
    "sharp": "^0.34.2",
    "solid-js": "^1.9.7",
    "tailwindcss": "^4.0.17"
  },
  "devDependencies": {
    "@qwik-ui/headless": "^0.6.5",
    "@types/eslint": "^8.56.10",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "eslint": "^8.57.0",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-qwik": "^1.5.2",
    "prettier": "^3.2.5",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-tailwindcss": "^0.6.11"
  }
}
